{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "quotes": "off",
      "indent": ["error", 4],
      "node/no-callback-literal": "off"
    }
}
